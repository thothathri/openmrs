<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs">

	<class name="org.openmrs.hl7.HL7OutQueueDestination" table="hl7_out_queue_destination">

		<id name="hoqdId" type="java.lang.Integer" column="hoqd_id" unsaved-value="0">
			<generator class="native">
				<param name="sequence">hl7_out_queue_destination_hoqd_id_seq</param>
			</generator>
		</id>

		<discriminator column="hoqd_id" insert="false" />
		
		<property name="name" type="java.lang.String" 
			column="name" not-null="true" length="255" />
		
		<property name="description" type="java.lang.String" 
			column="description" not-null="false" length="1024" />	
		
		<property name="destination" type="java.lang.String" 
			column="destination" not-null="true" length="255" />
			
 		<property name="dateCreated" type="java.util.Date" 
			column="date_created" not-null="false" length="19" />
			
		<many-to-one name="createdBy" class="org.openmrs.User" not-null="true">
			<column name="created_by" />
		</many-to-one>
		
		<property name="dateChanged" type="java.util.Date" 
			column="date_changed" not-null="false" length="19" />
			
		<many-to-one name="changedBy" class="org.openmrs.User" not-null="false">
			<column name="changed_by" />
		</many-to-one>	
		
		<property name="dateRetired" type="java.util.Date" 
			column="date_retired" not-null="false" length="19" />
			
		<property name="retired" type="java.lang.Boolean" column="retired"
			length="1" not-null="true" />
			
		<many-to-one name="retiredBy" class="org.openmrs.User" not-null="false">
			<column name="retired_by" />
		</many-to-one> 
		
		<property name="retireReason" type="java.lang.String" 
			column="retire_reason" not-null="false" length="255" />
		
	</class>
</hibernate-mapping>
